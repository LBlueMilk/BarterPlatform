@model BarterPlatform.Models.AuthenticationRecords

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>AuthenticationRecords</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="AuthID" />
            <div class="form-group">
                <label asp-for="Employee_EmpID" class="control-label"></label>
                <select asp-for="Employee_EmpID" class="form-control" asp-items="ViewBag.Employee_EmpID"></select>
                <span asp-validation-for="Employee_EmpID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Member_MemID" class="control-label"></label>
                <select asp-for="Member_MemID" class="form-control" asp-items="ViewBag.Member_MemID"></select>
                <span asp-validation-for="Member_MemID" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="AuthStatus" /> @Html.DisplayNameFor(model => model.AuthStatus)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="AuthTime" class="control-label"></label>
                <input asp-for="AuthTime" class="form-control" />
                <span asp-validation-for="AuthTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
